
<link rel="stylesheet" type="text/css" href="/stylesheets/login.css" />
<script src="/javascripts/board.js"></script>
        <div class="page-data">
            
            <form method="post" action="/gallery/add" enctype="multipart/form-data" >
                <h2>Select an image to upload:</h2>
                <br/>
                <input type="file" name="fileName">
                <br/>
                <br/>
            
                <table cellpadding="11">
                <tr>
                    <td class="label">제목</td><td>: <input type="text" name="name" required></td>
                 </tr>
                 <tr>
                    <td class="label" valign="top">내용</td><td valign="top">: <textarea name="address" required cols="30" rows="5"></textarea></td>
                </tr>
                <tr>
                    <td class="label">email</td><td>: <input type="text" name="email" required></td>
                 </tr>
                  <tr>
                    <td class="label">Phone</td><td>: <input type="text" name="phone" required></td>
                 </tr>
                 <tr>
                    <td class="label"></td>
                    <td>
                        &nbsp; <input type="submit" name="submit" value="Save">
                       <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelAdd()">
                    </td>
                 </tr>
                </table>
                <h1 id='uploadProgress'></h1>
            </form>
            
        </div>
