<link rel="stylesheet" type="text/css" href="stylesheets/signup.css" />
<div class="container">
    <section id="content">
        <div id=logo>
            <a href="#"><img src="images/logo.png" alt="Logo for Korean Travel"></a>
        </div>
       
        <form action="/signup" method="post">    
            <h1>Sign Up</h1>

            <div>
                <input type="text" placeholder="username" name="username" id="username" />
            </div>

            <div>
                <input type="password" placeholder="Password" name="password" id="password" />
            </div>
            <!-- <div>
                <input type="password" placeholder="Re-enter password" name="password2" id="password2" />
            </div> -->
            <div>
                <input type="email" placeholder="email" name="emil" id="emil" />
            </div>
            <div>
                <input id ="submit" type="submit" value="Sign Up" name="sign" />
                
            </div>
        </form>

    </section>

</div>
