<div class="main">
	<section id="content">
	<form method="post" action="/gallery/edit/<%=data[0].gallery_id%>" class="center-block">
		<div class="form-group">
			<label for="exampleInputEmail1">title:</label>
			<input type="text" name="name" value="<%=data[0].name%>">
        </div>  
        <label for="exampleInputEmail1">Content</label>
        <div class="center-block">
        <textarea name="info" required cols="30" rows="5"><%=data[0].info%></textarea>
      </div>
		<div class="center-block">
			<input type="submit" name="submit" value="search" style="margin-left: 50px">
			<input type="submit" name="cancel" value="Cancel" class="cancel" onclick="cancelAdd()">
		</div>
	</form>
	</section>
</div>
